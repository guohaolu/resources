(function($) {
    AJS.bind("init.rte", function() {
        $( document ).ready(function() {
            $.ajax({
                url: AJS.params.baseUrl+"/rest/latexmath/latest/showlatexmacro",
                dataType: "json"
            }).done(function (data) {
                $("body").toggleClass("showLatexFalse", !data.enabled);
            });          
        });
    });
})(AJS.$);

}catch(e){WRMCB(e)};
